{
  "name": "commands-stack",
  "version": "0.0.0-development",
  "description": "Based configured template with Semantic Release",
  "private": false,
  "main": "index.js",
  "scripts": {
    "commit": "cz",
    "test": "jest --detectOpenHandles",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "semantic"
  ],
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "*.{ts,tsx}": [
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^29.3.1",
    "@types/jest": "^29.2.4",
    "babel-jest": "^29.3.1",
    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "semantic-release": "^19.0.5",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vladislavkovaliov/commands-stack.git"
  }
}
